{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true
  },
  "rules": {
    "angular/controller-as-route": 0,
    "angular/controller-as": 0,
    "angular/module-getter": 0,
    "angular/controller-as-vm": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "angular/di": 0
  }
}
